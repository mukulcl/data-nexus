# ----- Database (Postgres) -----
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=data_nexus

# ----- ClickHouse -----
CLICKHOUSE_USER=default
CLICKHOUSE_PASSWORD=
CLICKHOUSE_DB=data_nexus

# ----- Redis -----
REDIS_PORT=6379

# ----- MinIO (S3-compatible) -----
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin
MINIO_PORT=9000
MINIO_CONSOLE_PORT=9001
S3_BUCKET_RAW=raw
S3_BUCKET_CLEAN=clean

# ----- Meilisearch -----
MEILI_MASTER_KEY=devmasterkey
MEILI_PORT=7700

# ----- API -----
API_PORT=8080
NODE_ENV=development

# Optional: ClickHouse HTTP host override for local tools
# CLICKHOUSE_HOST=http://localhost:8123
